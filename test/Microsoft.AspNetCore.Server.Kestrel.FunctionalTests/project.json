{
  "version": "1.1.0-*",
  "dependencies": {
    "dotnet-test-xunit": "2.2.0-*",
    "Microsoft.AspNetCore.Http.Abstractions": "1.2.0-*",
    "Microsoft.AspNetCore.Server.Kestrel.Https": "1.2.0-*",
    "Microsoft.AspNetCore.Testing": "1.2.0-*",
    "Microsoft.Extensions.Logging.Testing": "1.2.0-*",
    "Moq": "4.6.36-*",
    "Newtonsoft.Json": "9.0.1",
    "xunit": "2.2.0-*",
    "Microsoft.CodeCoverage": {
      "type": "build",
      "version": "1.0.1"
    }
  },
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.2.0-*",
          "type": "platform"
        },
        "System.Net.Http.WinHttpHandler": "4.4.0-*",
        "System.Net.NetworkInformation": "4.4.0-*",
        "System.Runtime.Serialization.Primitives": "4.4.0-*"
      },
      "imports": "dnxcore50"
    },
    "net451": {
      "frameworkAssemblies": {
        "System.Net.Http": "4.0.0.0"
      },
      "imports": "dnx451"
    }
  },
  "buildOptions": {
    "allowUnsafe": true,
    "compile": {
      "include": [
        "../shared/**/*.cs"
      ]
    },
    "keyFile": "../../tools/Key.snk",
    "copyToOutput": {
      "include": "TestResources/testCert.pfx"
    }
  },
  "testRunner": "xunit",
  "publishOptions": {
    "include": [
      "TestResources/testCert.pfx"
    ]
  }
}